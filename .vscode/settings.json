{
  "files.exclude": {
    "**/.idea": true,
    "**/.dart_tool": true,
    "**/.metadata": true,
    "**/.packages": true,
    "**/RESOURCES": true,
    "**/.vscode": true,
    "**/build": true,
    "**/lib_BACKUP": true,
    "**/*.iml": true,
    "*.flutter-plugins*": true,
  },
  "cSpell.enableFiletypes": [
    "!yaml","!dart"
  ],
  "peacock.color": "#832561",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#5b1a44",
    "activityBar.activeBorder": "#5d7c23",
    "activityBar.background": "#5b1a44",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#5d7c23",
    "activityBarBadge.foreground": "#e7e7e7",
    "panel.border": "#5b1a44",
    "sash.hoverBorder": "#5b1a44",
    "statusBar.background": "#5b1a44",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#832562",
    "statusBarItem.remoteBackground": "#5b1a44",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#5b1a44",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#5b1a4499",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
}